  "query": "\n            SELECT id, from_address, to_address, amount, fee, transaction_hash,\n                   block_number, status as \"status: TransactionStatus\", created_at\n            FROM transactions\n            WHERE from_address = $1 OR to_address = $1\n            ORDER BY created_at DESC\n            LIMIT $2 OFFSET $3\n            ",
